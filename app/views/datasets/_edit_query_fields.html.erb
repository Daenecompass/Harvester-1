<% @input.each do |field| %>
  <%= label_tag field[0].to_sym %>
  <% if field[1] == "string" %>
    <%= text_field_tag "input_query_fields[#{count}][#{field[0]}]", in_f[field[0]] %>
  <% elsif field[1] == "file" %>
    <%= text_field_tag "input_query_fields[#{count}][#{field[0]}]", in_f[field[0]] %>
  <% end %>
<% end %>
